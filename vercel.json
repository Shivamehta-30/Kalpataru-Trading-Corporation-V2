{
    "builds": [{
        "src": "contract_form/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    }],
    "routes": [
        {
            "rewrites": [{ "source": "/(.*)", "destination": "contract_form/wsgi.py" }]
        }
    ]
}